    "@types/node": "^13.13.1",
    "@types/react": "^16.9.43",
    "@urql/core": "^1.11.7",
    "@xstate/react": "^1.0.1",
    "autoprefixer": "^9.7.6",
    "codemirror": "^5.59.1",
    "dotenv": "8.2.0",
    "fast-glob": "^3.2.4",
    "final-form": "^4.18.2",
    "formik": "^2.1.4",
    "graphiql": "^1.2.2",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.11.0",
    "js-cookie": "^2.2.1",
    "next": "10.0.5",
